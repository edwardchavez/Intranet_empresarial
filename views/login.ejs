      <img src="images/logo.png" alt="ARS FUTURO"><br><br><br>
      <h6 style="color: #fff;">BIENVENIDO AL INTRANET</h6>
    </div>

    
    <form class="login-form"   onsubmit="return validate()" method="post" > <!-- onsubmit="return validate()"    action="/login"-->
       <h3>Usuario:</h3>
      <input type="text" id="user_name" name="user_name" placeholder="Username" />
      <br>
      <h3>Contrase√±a:</h3>
      <input type="password" id="password" name="password" placeholder="Password" />
      <br>
      <input type="submit" value="Login" class="login-button" />
      <br>
      <div>
        <a class="sign-up" href="/signup">Registrate!</a>
      </div>
      <!-- <br>
      <h6 class="no-access">Can't access your account?</h6>-->
     
    </form>
      
    
